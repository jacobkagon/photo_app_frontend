{
  "env": { "browser": true, "es2021": true, "node": true },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "eslint-config-prettier"
  ],
  "rules": {
    "strict": ["error", "never"]
  }
}
